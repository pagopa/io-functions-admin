{
    "bindings": [
      {
        "type": "cosmosDBTrigger",
        "name": "documents",
        "direction": "in",
        "leaseCollectionName": "change-feed-leases",
        "leaseCollectionPrefix": "userDataProcessing",
        "connectionStringSetting": "COSMOSDB_CONNECTION_STRING",
        "databaseName": "%COSMOSDB_NAME%",
        "collectionName": "user-data-processing",
        "createLeaseCollectionIfNotExists": true
      },
      {
        "name": "starter",
        "type": "orchestrationClient",
        "direction": "in"
      },
      {
        "connection": "FailedUserDataProcessingStorageConnection",
        "tableName": "%FAILED_USER_DATA_PROCESSING_TABLE%",
        "name": "FailedUserDataProcessingIn",
        "type": "table",
        "direction": "in"
      },
      {
        "connection": "FailedUserDataProcessingStorageConnection",
        "tableName": "%FAILED_USER_DATA_PROCESSING_TABLE%",
        "name": "FailedUserDataProcessingOut",
        "type": "table",
        "direction": "out"
      }
    ],
    "scriptFile": "../dist/UserDataProcessingTrigger/index.js"
}