{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "adm/services/{serviceId}/logo",
      "methods": [
        "put"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "name": "logo",
      "path": "$web/services/{serviceId}.png",
      "connection": "AssetsStorageConnection",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/UploadServiceLogo/index.js"
}