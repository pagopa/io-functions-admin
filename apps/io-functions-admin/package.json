{
  "name": "io-functions-admin",
  "version": "2.0.2",
  "license": "MIT",
  "scripts": {
    "clean": "shx rm -rf dist",
    "clean:generated": "shx rm -rf ./generated",
    "build": "tsc && yarn postbuild",
    "postbuild": "dependency-check package.json --no-dev --missing ./dist/**/*.js",
    "build:watch": "tsc -w",
    "typecheck": "tsc --noemit",
    "extensions:install": "func extensions install",
    "start": "dotenv -e .env func start --javascript",
    "start:watch": "yarn start watch",
    "test": "vitest run",
    "test:coverage": "yarn test --coverage",
    "lint": "yarn lint:check --fix",
    "lint:check": "eslint . -c eslint.config.mjs",
    "lint-api": "oval validate -p openapi/index.yaml",
    "dist:modules": "modclean -r -n default:safe && yarn install --production",
    "dist:zip": "zip -r dist.zip . --exclude @.funcignore --exclude .funcignore",
    "predeploy": "npm-run-all generate build dist:modules",
    "generate": "shx rm -rf ./generated/definitions && shx mkdir -p ./generated/definitions && gen-api-models --api-spec ./openapi/index.yaml --out-dir ./generated/definitions",
    "preversion": "auto-changelog  --config .auto-changelog.json --unreleased --commit-limit false --stdout --template preview.hbs",
    "version": "auto-changelog -p --config .auto-changelog.json --unreleased && git add CHANGELOG.md"
  },
  "description": "",
  "devDependencies": {
    "@azure/functions": "^3.5.0",
    "@pagopa/eslint-config": "^5.0.0",
    "@pagopa/openapi-codegen-ts": "^10.0.5",
    "@pagopa/typescript-config-node": "*",
    "@types/documentdb": "^1.10.8",
    "@types/express": "^4.17.12",
    "@types/html-to-text": "^5.1.1",
    "@types/node": "~20.12.2",
    "@types/nodemailer": "^4.6.8",
    "@types/upng-js": "^2.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "auto-changelog": "^2.3.0",
    "azure-functions-core-tools": "^4.0.5455",
    "azurite": "^3.13.1",
    "dependency-check": "^4.1.0",
    "eslint": "^9.35.0",
    "fast-check": "^1.26.0",
    "marked": "^3.0.2",
    "modclean": "^3.0.0-beta.1",
    "npm-run-all": "^4.1.5",
    "oval": "^1.0.0",
    "prettier": "^3.6.2",
    "shx": "^0.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^8.10.2",
    "typescript": "5.7.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@azure/arm-apimanagement": "^9.1.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/data-tables": "^13.2.2",
    "@azure/identity": "^4.2.0",
    "@azure/ms-rest-js": "^2.7.0",
    "@pagopa/express-azure-functions": "^4.0.1",
    "@pagopa/handler-kit": "^1.1.0",
    "@pagopa/handler-kit-azure-func": "^1.2.1",
    "@pagopa/io-functions-commons": "^29.4.0",
    "@pagopa/logger": "^1.0.1",
    "@pagopa/ts-commons": "^13.1.2",
    "@types/archiver": "^3.1.1",
    "@types/randomstring": "^1.1.6",
    "@xmldom/xmldom": "^0.7.13",
    "applicationinsights": "^2.9.8",
    "archiver": "^4.0.2",
    "archiver-zip-encrypted": "^1.0.8",
    "azure-storage": "^2.10.4",
    "body-parser": "^1.19.0",
    "documentdb": "^1.12.2",
    "dotenv": "^17.2.3",
    "durable-functions": "^1.4.6",
    "express": "^4.17.1",
    "fp-ts": "^2.16.5",
    "html-to-text": "^5.1.1",
    "io-ts": "^2.2.21",
    "node-fetch": "^2.6.1",
    "nodemailer": "^7.0.0",
    "nodemailer-sendgrid": "^1.0.3",
    "randomstring": "^1.2.1",
    "redis": "^3.1.2",
    "redis-clustr": "^1.7.0",
    "ulid": "^2.3.0",
    "upng-js": "^2.1.0",
    "winston": "^3.3.3",
    "xpath": "^0.0.32",
    "yaml": "^1.10.2"
  },
  "resolutions": {
    "handlebars": "~4.5.3"
  }
}
